        </Grid.ColumnDefinitions>
        <!--<Button Content="Перейти к следующей стадии расчета!" HorizontalAlignment="Left" Height="28" Margin="874,546,0,0" VerticalAlignment="Top" Width="304" Click="Button_Click"/>-->
        <Grid>
